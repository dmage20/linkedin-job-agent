# Staging Environment Configuration
# LinkedIn Job Agent - Database System (US-001)

# Anthropic API Configuration
ANTHROPIC_API_KEY=your_anthropic_api_key_here

# Application Settings - Staging
MAX_APPLICATIONS_PER_DAY=5
SEARCH_INTERVAL_MINUTES=120
RATE_LIMIT_SECONDS=10

# Database Configuration - Staging
DATABASE_URL=sqlite:///./staging/data/db/jobs_staging.db
DATABASE_ENV=staging

# Logging Configuration - Staging
LOG_LEVEL=DEBUG
LOG_FILE=./staging/logs/app_staging.log
ENABLE_SQL_LOGGING=true

# Performance Monitoring
ENABLE_PERFORMANCE_MONITORING=true
PERFORMANCE_LOG_FILE=./staging/logs/performance_staging.log

# Health Check Configuration
HEALTH_CHECK_ENABLED=true
HEALTH_CHECK_INTERVAL=30

# Notifications - Disabled in staging
ENABLE_NOTIFICATIONS=false
NOTIFICATION_SOUND=false

# Browser Settings - Staging
HEADLESS_MODE=true
BROWSER_TIMEOUT_SECONDS=45

# Environment Identifier
ENVIRONMENT=staging
DEPLOYMENT_TIMESTAMP=2025-09-23T11:25:00Z